{
  "name": "@nyarthan/prettier-config",
  "version": "1.0.3",
  "description": "Sharable prettier configuration",
  "author": "Jannis Morgenstern <jannis.n.morgenstern@gmail.com> (https://github.com/nyarthan)",
  "license": "MIT",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "homepage": "https://github.com/nyarthan/prettier-config?tab=readme-ov-file",
  "repository": "github:nyarthan/prettier-config",
  "bugs": "https://github.com/nyarthan/prettier-config/issues",
  "packageManager": "pnpm@10.15.1",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "24.9.0",
      "onFail": "error"
    }
  },
  "type": "module",
  "imports": {
    "#package.json": "./package.json"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/src/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "build": "rslib build",
    "test": "node test/snapshots.test.ts",
    "test:u": "node --test-update-snapshots test/snapshots.test.ts",
    "release": "pnpm publish --access public --provenance",
    "prerelease": "pnpm build"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/format": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@rslib/core": "0.15.0",
    "@types/node": "24.3.0",
    "conventional-changelog-atom": "5.0.0",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": ">=4",
    "prettier": ">=3",
    "prettier-plugin-astro": ">=0.14",
    "prettier-plugin-tailwindcss": ">=0.6"
  },
  "peerDependenciesMeta": {
    "@ianvs/prettier-plugin-sort-imports": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-tailwindcss": {
      "optional": true
    }
  }
}
