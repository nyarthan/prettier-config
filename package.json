{
  "name": "@nyarthan/prettier-config",
  "version": "0.1.0",
  "description": "Sharable prettier configuration",
  "author": "Jannis Morgenstern <jannis.n.morgenstern@gmail.com> (https://github.com/nyarthan)",
  "license": "MIT",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "packageManager": "pnpm@10.8.0",
  "type": "module",
  "imports": {
    "#package.json": "./package.json"
  },
  "scripts": {
    "prettier": "NODE_OPTIONS='--disable-warning ExperimentalWarning --experimental-strip-types' prettier",
    "changeset": "NODE_OPTIONS='--disable-warning ExperimentalWarning --experimental-strip-types' changeset",
    "format:check": "pnpm prettier --check .",
    "format": "pnpm prettier --write ."
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3"
  }
}
